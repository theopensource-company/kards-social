    "next": "13.0.0",
    "next-sitemap": "^3.1.30",
    "query-string": "^7.1.1",
    "ra-core": "^4.4.4",
    "ra-data-json-server": "^4.4.4",
    "react": "18.2.0",
    "react-admin": "^4.5.0",
    "react-admin-json-view": "^2.0.0",
    "react-dom": "18.2.0",
