{
  "name": "kards-app",
  "version": "0.1.0",
  "description": "Frontend app for kards social. Build with next as a PWA.",
  "author": "The Open Source Company",
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "NEXT_PUBLIC_ENV=\"dev\" NEXT_PUBLIC_SURREAL_ENDPOINT=\"https://dev-db.kards.social\" NEXT_PUBLIC_SURREAL_NAMESPACE=\"micha-test\" next dev -p 12010",
    "build": "next build && next export && next-sitemap && pnpm build-storybook",
    "start": "next start",
    "lint": "eslint . --ext ts --ext tsx --max-warnings 0",
    "postbuild": "next-sitemap",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public -o out/developers/storybook --modern --docs"
  },
